class Solution {
public:
    void nextPermutation(vector<int>& nums) {
        int n=nums.size();
        int k=n-2;
        while(k>=0){
            for(int i=k+1;i<=n-1;i++){
                int min=INT_MAX;
                if(a[k]<a[i]){
                   if(a[i]<min){
                       min=a[i];
                   }
                }
            }
            k--;
        }
    }
};
